/files:
  displayName: Court File Resources

  /courtCalendarDetailsByDay:
    get:
      description: |
        Gets the court list details from JUSTIN web services.
      headers:
        requestAgencyIdentifierId:
          description: Requesting Agency Identifier
          type: string
          required: true
        requestPartId:
          description: Requesting Participant Id
          type: string
          required: true
        applicationCd:
          description: The code for the calling application
          type: string
          required: true
      queryParameters:
        agencyCode:
          description: Agency Code; for example 83.0001 (Kelowna).
          type: string
          required: true
        appearanceDt: 
          description: The appearance date in the format YYYY-MM-dd
          type: string
          required: true
        roomCd:
          description: Court room code
          type: string
          required: true
      responses:
        200:
          body:
            application/json:
              schema: courtCalendarDetailByDay
        404:
          body:
            application/json:
              example: |
                {"message": "No items matching the given criteria were found."}
              
  /courtlist:
    get:
      description: |
        Gets the court list from the CCD web services.
      headers:
        requestAgencyIdentifierId:
          description: Requesting Agency Identifier
          type: string
          required: true
        requestPartId:
          description: Requesting Participant Id
          type: string
          required: true
        applicationCd:
          description: The code for the calling application
          type: string
          required: true
      queryParameters:
        agencyId:
          displayName: agencyId
          description: Agency Identifier Code (Location Code); for example 4801 (Kelowna).
          type: string
          required: true
        roomCd:
          displayName: roomCd
          type: string
          required: true
        proceedingDt:
          displayName: proceedingDt
          description: The proceeding date in the format YYYY-MM-dd
          type: string
          required: true
        divisionCd:
          displayName: divisionCd
          description: The division code; CR, or CV.
          type: string
          required: false
        fileNumber:
          displayName: fileNumber
          description: The full file number; for example 1500-3
          type: string
          required: false
      responses:
        200:
          body:
            application/json:
              schema: courtList
        404:
          body:
            application/json:
              example: |
                {"message": "No items matching the given criteria were found."}

  /recordOfProceedings:
    get:
      description: |
        Gets the Record Of Proceedings for a given file.
      headers:
        requestAgencyIdentifierId:
          description: Requesting Agency Identifier
          type: string
          required: true
        requestPartId:
          description: Requesting Participant Id
          type: string
          required: true
        applicationCd:
          description: The code for the calling application
          type: string
          required: true
      queryParameters:
        partId:
          displayName: partId
          description: The participant id associated to the Record Of Proceedings.
          type: string
          required: true
        profSeqNo:
          displayName: profSeqNo
          type: string
          required: true
        courtLevelCd:
          displayName: courtLevelCd
          description: The associated court level code.
          type: string
          enum: [ "P", "S", "A" ]
          required: true
        courtClassCd:
          displayName: courtClassCd
          type: string
          enum: [ "A", "Y", "T", "F", "C", "M", "L", "R", "B", "D", "E", "G", "H", "N", "O", "P", "S", "V" ]
          required: true
      responses:
        200:
          body:
            application/json:
              schema: ropResponse
        404:
          body:
            application/json:
              example: |
                {"message": "No items matching the given criteria were found."}

  /document:
    get:
      description: |
        Gets the document identified by a document id.
      headers:
        requestAgencyIdentifierId:
          description: Requesting Agency Identifier
          type: string
          required: true
        requestPartId:
          description: Requesting Participant Id
          type: string
          required: true
        applicationCd:
          description: The code for the calling application
          type: string
          required: true
      queryParameters:
        documentId:
          displayName: documentId
          description: cfcAccusedFile.document array element's imageId, or cvfcCivilFile document array element's imageId
          type: string
          required: true
        courtDivisionCd:
          displayName: courtDivisionCd
          description: R for criminal, I for non-criminal
          type: string
          required: true
        fileId:
          description: Physical File Id (CEIS) or Justin No (JUSTIN)
          type: string
          required: true
      responses:
        200:
          body:
            application/json:
              schema: documentResponse
        404:
          body:
            application/json:
              example: |
                {"message": "No items matching the given criteria were found."}

